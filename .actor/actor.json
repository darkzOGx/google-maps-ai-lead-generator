{
  "actorSpecification": 1,
  "name": "google-maps-business-email-scraper",
  "title": "Google Maps Business Email Scraper & B2B Lead Generator",
  "version": "2.1",
  "description": "Scrape Google Maps for verified B2B leads with emails, phone numbers & addresses. AI-powered lead scoring. Export to CSV/Excel. Perfect for sales prospecting & agencies.",
  "categories": ["BUSINESS", "ECOMMERCE", "MARKETING"],
  "dockerfile": "./Dockerfile",
  "readme": "./README.md",
  "input": "./input_schema.json",
  "meta": {
    "templateId": "b2b-lead-generator",
    "minMemoryMbytes": 4096,
    "recommendedMemoryMbytes": 8192,
    "maxMemoryMbytes": 32768
  },
  "storages": {
    "dataset": {
      "actorSpecification": 1,
      "title": "Enriched B2B Leads",
      "description": "Sales-ready leads with validated emails, company data, and quality scores",
      "views": {
        "overview": {
          "title": "All Leads",
          "transformation": {
            "fields": [
              "businessName",
              "email",
              "phone",
              "website",
              "leadGrade",
              "leadScore",
              "rating",
              "reviewCount",
              "address",
              "category"
            ]
          },
          "display": {
            "component": "table",
            "properties": {
              "businessName": { "label": "Business Name", "format": "text" },
              "email": { "label": "Email", "format": "text" },
              "phone": { "label": "Phone", "format": "text" },
              "website": { "label": "Website", "format": "link" },
              "leadGrade": { "label": "Grade", "format": "text" },
              "leadScore": { "label": "Score", "format": "number" },
              "rating": { "label": "Rating", "format": "number" },
              "reviewCount": { "label": "Reviews", "format": "number" }
            }
          }
        },
        "high-quality": {
          "title": "High-Quality Leads (A+ & A)",
          "transformation": {
            "fields": [
              "businessName",
              "email",
              "phone",
              "website",
              "leadGrade",
              "leadScore"
            ]
          },
          "display": {
            "component": "table"
          }
        }
      }
    }
  }
}
